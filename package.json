{
  "name": "model-selector",
  "version": "0.1.0",
  "description": "An open source assistant to help you choose, adapt, and deploy the best AI model for your task",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "dev:vanilla": "vite --config vite.config.vanilla.js",
    "build:vanilla": "vite build --config vite.config.vanilla.js",
    "preview:vanilla": "vite preview --config vite.config.vanilla.js",
    "test": "vitest",
    "update-models": "node src/lib/aggregation/cli.js",
    "update-models:dry-run": "node src/lib/aggregation/cli.js --dry-run"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.5"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "svelte": "^4.2.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "ai",
    "ml",
    "model-selection",
    "pwa",
    "offline",
    "svelte"
  ],
  "author": "Model Selector Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/model-selector.git"
  }
}
